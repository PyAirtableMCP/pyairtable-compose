# Airtable Gateway Configuration

# Service
SERVICE_NAME=airtable-gateway
SERVICE_VERSION=1.0.0
HOST=0.0.0.0
PORT=8002

# Airtable
AIRTABLE_TOKEN=your-airtable-pat-token
AIRTABLE_RATE_LIMIT=5
AIRTABLE_TIMEOUT=30

# Redis
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# Security
INTERNAL_API_KEY=internal-api-key

# Logging
LOG_LEVEL=INFO

# CORS
CORS_ORIGINS=["*"]