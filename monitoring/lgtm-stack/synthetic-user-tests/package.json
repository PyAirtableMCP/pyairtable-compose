{
  "name": "pyairtable-synthetic-tests",
  "version": "1.0.0",
  "description": "Comprehensive synthetic user tests for PyAirtable platform",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:codegen": "playwright codegen",
    "report": "playwright show-report",
    "install-browsers": "playwright install",
    "start-agents": "node src/orchestrator/test-orchestrator.js",
    "monitor": "node src/monitoring/metrics-collector.js",
    "dashboard": "node src/dashboard/test-dashboard.js"
  },
  "keywords": [
    "playwright",
    "synthetic-tests",
    "user-agents",
    "pyairtable",
    "automation",
    "monitoring"
  ],
  "dependencies": {
    "@playwright/test": "^1.40.0",
    "faker": "^6.6.6",
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "express": "^4.18.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "prometheus-api-metrics": "^3.2.0",
    "prom-client": "^15.1.0",
    "node-fetch": "^3.3.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}