{
  "dashboard": {
    "id": null,
    "title": "PyAirtable Services - Real Observability",
    "tags": ["pyairtable", "observability", "sre"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 39,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"ai-processing-service|airtable-gateway|llm-orchestrator\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate (req/min)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m]) * 60",
            "legendFormat": "{{job}} - {{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 3,
        "title": "Request Duration P95 (ms)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "{{job}} - {{endpoint}} P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "{{job}} - {{endpoint}} P50",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "HTTP Status Codes",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m]) * 60",
            "legendFormat": "{{job}} - {{status}}xx",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "Active Connections",
        "type": "timeseries",
        "targets": [
          {
            "expr": "active_connections",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Error Rate (%)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "(rate(http_requests_total{status=~\"4..|5..\"}[5m]) / rate(http_requests_total[5m])) * 100",
            "legendFormat": "{{job}} Error Rate",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Request Volume by Endpoint",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by(endpoint) (increase(http_requests_total[1h]))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 8,
        "title": "Golden Signals Summary",
        "type": "table",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m]) * 60",
            "format": "table",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "transformations": [
          {
            "id": "groupBy",
            "options": {
              "fields": {
                "job": {"aggregations": [], "operation": "groupby"},
                "endpoint": {"aggregations": [], "operation": "groupby"},
                "Value": {"aggregations": ["lastNotNull"], "operation": "aggregate"}
              }
            }
          }
        ]
      }
    ]
  },
  "inputs": [],
  "requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    }
  ]
}